<?xml version="1.0" encoding="utf-8"?>
<ScrollView>

    <TableLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" >

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="Enter" />

            <TextView
                style="@style/code"
                android:text=". - . -" />

            <TextView
                style="@style/symbol"
                android:text="(" />

            <TextView
                style="@style/code_nopadding"
                android:text="- . - - ." />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="&apos;" />

            <TextView
                style="@style/code"
                android:text=". - - - - ." />

            <TextView
                style="@style/symbol"
                android:text=")" />

            <TextView
                style="@style/code_nopadding"
                android:text="- . - - . -" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="\@" />

            <TextView
                style="@style/code"
                android:text=". - - . - ." />

            <TextView
                style="@style/symbol"
                android:text="." />

            <TextView
                style="@style/code_nopadding"
                android:text=". - . - . -" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="&amp;" />

            <TextView
                style="@style/code"
                android:text=". - . . ." />

            <TextView
                style="@style/symbol"
                android:text="+" />

            <TextView
                style="@style/code_nopadding"
                android:text=". - . - ." />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text=":" />

            <TextView
                style="@style/code"
                android:text="- - - . . ." />

            <TextView
                style="@style/symbol"
                android:text="\?" />

            <TextView
                style="@style/code_nopadding"
                android:text=". . - - . ." />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="," />

            <TextView
                style="@style/code"
                android:text="- - . . - -" />

            <TextView
                style="@style/symbol"
                android:text="&quot;" />

            <TextView
                style="@style/code_nopadding"
                android:text=". - . . - ." />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="$" />

            <TextView
                style="@style/code"
                android:text=". . . - . . -" />

            <TextView
                style="@style/symbol"
                android:text=";" />

            <TextView
                style="@style/code_nopadding"
                android:text="- . - . - ." />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="=" />

            <TextView
                style="@style/code"
                android:text="- . . . -" />

            <TextView
                style="@style/symbol"
                android:text="/" />

            <TextView
                style="@style/code_nopadding"
                android:text="- . . - ." />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="!" />

            <TextView
                style="@style/code"
                android:text="- - - ." />

            <TextView
                style="@style/symbol"
                android:text="_" />

            <TextView
                style="@style/code"
                android:text=". . - - . -" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/cheatsheetrow" >

            <TextView
                style="@style/symbol"
                android:text="-" />

            <TextView
                style="@style/code"
                android:text="- . . . . -" />
        </TableRow>
    </TableLayout>

</ScrollView>